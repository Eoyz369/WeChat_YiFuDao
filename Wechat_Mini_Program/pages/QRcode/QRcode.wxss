.page {
  background: #7ec37b;
  background-position: 0 200rpx;
  background-repeat: no-repeat;
  background-size: 750rpx 388rpx;
  min-height: 100vh;
  padding-top: 1px;
  background-image: url(data:image/png;base64,);
}

.now_time {
  background-color: #fff;
}

.lists {
  background: #f8f8f8;
  border-radius: 8rpx;
  margin: 30rpx;
  padding: 12rpx;
}

.list {
  align-items: center;
  /* 自己加 */
  font-size: 25rpx;
  display: flex;
}

.list .list_left {
  padding: 12rpx 24rpx;
  width: 200rpx;
}

.list .list_right {
  flex: 1;
  padding: 12rpx 24rpx 12rpx 0;
}

.list:last-child .list_right {
  border-bottom: none;
}

.tips {
  color: #fff;
  font-size: 24rpx;
  padding: 0 30rpx 60rpx;
}

.code {
  border: 1px solid #7ec37b;
  background-color: #fff;
  border-radius: 20rpx;
  margin: 40rpx auto;
  overflow: hidden;
}

.code,.code image {
  height: 300rpx;
  width: 300rpx;
}

#QR{
  height: 265rpx;
  margin: 9px;
  width: 265rpx;
}

.now_time {
  border: 1rpx solid #7ec37b;
  border-radius: 44rpx;
  color: #7ec37b;
  font-size: 48rpx;
  margin: 30rpx auto;
  padding: 10rpx 30rpx;
  text-align: center;
  width: 288px;
}

.lists_bg {
  background-position: 100%;
  background-repeat: no-repeat;
  background-size: 182rpx 144rpx;
  background-image: url(data:image/png;base64,);
}

.no_data {
  background: #7ec37b;
  border-radius: 16rpx;
  color: #b7b7b7;
  margin: 30rpx;
  padding: 30rpx;
  text-align: center;
}



.ani {
  animation: float 3s linear 0s infinite;
  background-image: url(https://meeting-image-1257318859.cos.ap-beijing.myqcloud.com/htmlImages/yfd/student/ani-balloon.png);
  background-position: center 100%;
  background-repeat: no-repeat;
  background-size: 750rpx 510rpx;
  min-height: 100vh;
  position: absolute;
  
  top: 0;
  width: 100%;
}

@-webkit-keyframes float {
  0% {
      background-position: center 50%;
  }

  100% {
      background-position: center -510rpx;
  }
}

@keyframes float {
  0% {
      background-position: center 50%;
  }

  100% {
      background-position: center -508rpx;
  }
}
