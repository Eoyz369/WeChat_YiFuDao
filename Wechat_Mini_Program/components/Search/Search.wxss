.search {
    padding: 30rpx;
}

.search_form {
    align-items: center;
    background-color: #f8f8f8;
    border-radius: 16rpx;
    box-sizing: border-box;
    display: flex;
    padding: 15rpx;
    position: relative;
}

.search_form input {
    flex: 1;
    z-index: 0;
}

.icon_search {
    background: url(https://meeting-image-1257318859.cos.ap-beijing.myqcloud.com/htmlImages/yfd/student/icon/icon-search.png) no-repeat;
    background-size: 100% 100%;
    height: 26rpx;
    width: 26rpx;
}

.icon_clear,.icon_search {
    margin-left: 10rpx;
    margin-right: 10rpx;
}

.icon_clear {
    background: url(https://meeting-image-1257318859.cos.ap-beijing.myqcloud.com/htmlImages/yfd/student/icon/icon-del.png) no-repeat;
    background-size: 100% 100%;
    height: 34rpx;
    width: 34rpx;
}