.radio_box {
  align-items: center;
  color: #717771;
  display: flex;
  margin-left: 30rpx;
  margin-right: 80rpx;
}

.radio_box:last-child {
  margin-bottom: 0;
}

.radio_box .label_radio {
  background-color: #fff;
  border: 5rpx solid #cecece;
  border-radius: 50%;
  height: 32rpx;
  margin-right: 24rpx;
  width: 32rpx;
}

.radio_box.checked .label_radio {
  align-items: center;
  background-color: #fff;
  border: 5rpx solid #7EC37B;
  display: flex;
  justify-content: center;
  position: relative;
}

.radio_box.checked .label_radio::before {
  background-color: #7EC37B;
  border-radius: 50%;
  content: "";
  display: block;
  height: 65%;
  width: 65%;
}

.radio_box .label_text {
  color: #999;
  font-size: 32rpx;
}

.radio_box.checked .label_text {
  color:#7EC37B;
}

.botton_area {
