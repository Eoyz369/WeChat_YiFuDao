}

.botton_area .botton_item,.botton_area .botton_item_grey {
  border-radius: 48rpx;
  color: #fff;
  font-size: 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  margin: 40rpx auto 80rpx;
  text-align: center;
  vertical-align: middle;
  /* background-color: rgb(194, 18, 18); */
  width: 324rpx;
}

.botton_area .botton_item_grey {
